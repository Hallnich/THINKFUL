##### Question 1 #####

SELECT 
	zip,
	MAX(maxtemperaturef) as maximum_temp
FROM
    weather
GROUP by zip
LIMIT 1;

##### Question 2 #####

SELECT 
	start_station,
	COUNT(start_station) as trips_started
FROM
  trips
GROUP by start_station
ORDER by trips_started DESC;

##### Question 3 #####

SELECT 
	start_station,
	COUNT(start_station) as trips_started
FROM
  trips
GROUP by start_station
ORDER by trips_started ASC
LIMIT 1;

##### Question 4 #####

SELECT 
	end_station,
	AVG(duration) AS average_duration
FROM 
	trips
GROUP by end_station;






